{
  "version": 1,
  "canisters": {
    "backend": {
      "type": "motoko",
      "main": "backend/main.mo"
    },
    "queue": {
      "type": "motoko",
      "main": "backend/queue.mo"
    },
    "frontend": {
      "type": "assets",
      "source": ["frontend/dist"],
      "dependencies": ["backend", "queue"]
    }
  },
  "defaults": {
    "build": {
      "packtool": "mops sources"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8080"
    }
  }
}